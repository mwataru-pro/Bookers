<h1>Books</h1>
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Body</th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>title</td>
			<td>body</td>
			<td>show</td>
			<td>edit</td>
			<td>destory</td>
		</tr>
	</tbody>
</table>

<h3>New book</h3>

<%= form_for(@book) do |f| %>

<h4>Title</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>

<br><%= f.submit 'Create book' %>

<% end %>